    "graphql-server-express": "^0.6.0",
    "graphql-tag": "^1.2.4",
    "graphql-tools": "^0.10.0",
    "hat": "^0.0.3",
    "knex": "^0.12.6",
    "lru-cache": "^4.0.2",
    "memoizejs": "^0.1.1",
    "morgan": "^1.8.1",
    "mz": "^2.6.0",
    "next": "next",
    "pg": "^6.1.2",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-apollo": "^0.10.0",
